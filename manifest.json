{
  "name": "Covfefe",
  "description": "Adds sassy comments to score! Converts kph to mph for Cricinfo. Author: Debmalya Sinha. debmalya.01@gmail.com",
  "manifest_version": 2,
  "version": "0.1.2",
  "icons": { "128": "icon.jpg" },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_title": "Covfefe",
    "default_icon": "icon.png"
    
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }

}